  "workflow_examples": {
    "simple_tts": {
      "description": "TTS อย่างง่าย",
      "nodes": [
        "Webhook Trigger",
        "HTTP Request (TTS)",
        "Save File"
      ],
      "webhook_body": {
        "text": "สวัสดีครับ นี่คือการทดสอบ TTS"
      }
    },
    "tts_with_rvc": {
      "description": "TTS + RVC Voice Conversion",
      "nodes": [
        "Form Trigger",
