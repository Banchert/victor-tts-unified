{
  "examples": {
    "tts_only": {
      "description": "TTS อย่างเดียว",
      "endpoint": "/tts",
      "url": "http://host.docker.internal:6969/tts",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "text": "{{ $json.text }}",
        "voice": "th-TH-PremwadeeNeural",
        "speed": 1.0
      }
    },
    "tts_rvc_unified": {
      "description": "TTS + RVC (Unified Endpoint)",
      "endpoint": "/unified",
      "url": "http://host.docker.internal:6969/unified",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "text": "{{ $json.text }}",
        "tts_voice": "th-TH-PremwadeeNeural",
        "speed": 1.0,
        "enable_rvc": true,
        "rvc_params": {
          "model_name": "VANXAI",
          "transpose": 0,
          "index_ratio": 0.75,
          "f0_method": "rmvpe"
        }
      }
    },
    "tts_rvc_full": {
      "description": "TTS + RVC (Full TTS Endpoint)",
      "endpoint": "/full_tts",
      "url": "http://host.docker.internal:6969/full_tts",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "text": "{{ $json.text }}",
        "tts_voice": "th-TH-PremwadeeNeural",
        "tts_speed": 1.0,
        "enable_rvc": true,
        "rvc_model": "VANXAI",
        "rvc_transpose": 0,
        "rvc_index_ratio": 0.75,
        "rvc_f0_method": "rmvpe"
      }
    },
    "voice_conversion_only": {
      "description": "Voice Conversion อย่างเดียว",
      "endpoint": "/voice_conversion",
      "url": "http://host.docker.internal:6969/voice_conversion",
      "method": "POST",
      "headers": {
        "Content-Type": "multipart/form-data"
      },
      "body": {
        "audio": "{{ $binary.audio_file }}",
        "request_data": "{\"model_name\":\"VANXAI\",\"transpose\":0,\"index_ratio\":0.75,\"f0_method\":\"rmvpe\"}"
      }
    }
  },
