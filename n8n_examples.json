      ]
    }
  },
  "error_handling": {
    "connection_refused": {
      "error": "The service refused the connection",
      "solution": "ตรวจสอบ URL และสถานะ TTS Server",
      "check_urls": [
        "http://localhost:6969/health",
        "http://host.docker.internal:6969/health"
      ]
    },
    "field_required": {
      "error": "Field required",
      "solution": "ตรวจสอบ JSON Body ให้ตรงกับ API Schema",
      "common_mistakes": {
        "tts_endpoint": "ใช้ 'voice' ไม่ใช่ 'tts_voice'",
        "unified_endpoint": "ใช้ 'tts_voice' ไม่ใช่ 'voice'"
      }
    },
    "model_not_found": {
      "error": "RVC model not found",
      "solution": "ตรวจสอบชื่อ RVC model ที่ API endpoint /models",
